{
  "name": "@senti/_underline",
  "version": "0.4.0",
  "description": "A GUI library for humans, inspired by DearImGui and CSS",
  "author": "Cristian Cornea",
  "email": "tinytales@mailbox.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senti-studio/_underline.git"
  },
  "keywords": [
    "pixijs",
    "game",
    "senti"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "examples",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "report": "npx vite preview --outDir html",
    "preview": "npx vite preview --outDir html",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@types/rewire": "^2.5.30",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "jsdom": "^23.2.0",
    "prettier": "3.1.1",
    "rewire": "^7.0.0",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitepress": "^1.0.0-rc.39",
    "vitest": "^1.1.0",
    "vitest-canvas-mock": "^0.3.3"
  },
  "dependencies": {
    "pixi.js": "^7.3.2"
  }
}